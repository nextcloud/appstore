{
    "extends": "tslint:recommended",
    "rules": {
        "no-any": true,
        "no-duplicate-variable": true,
        "indent": [true, "spaces"],
        "quotemark": [true, "single"],
        "max-line-length": [true, 120],
        "prefer-const": true,
        "no-console": ["log", {"allow": ["warn", "error"]}]
    }
}
