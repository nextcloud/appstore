<?xml version="1.0"?>
<info xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>{{ app.id }}</id>
    <name>{{ app.name }}</name>
    <summary>My first Nextcloud app</summary>
    <description>My first Nextcloud app</description>
    <licence>agpl</licence>
    <author mail="{{ app.author_mail }}" homepage="{{ app.author_homepage }}">{{ app.author_name }}</author>
    <version>0.0.1</version>
    <namespace>{{ app.namespace }}</namespace>
    <category>tools</category>
    <dependencies>
        <owncloud min-version="9.2"/>
        <nextcloud min-version="11"/>
    </dependencies>
</info>
